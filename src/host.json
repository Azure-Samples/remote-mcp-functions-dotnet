{
    "version": "2.0",
    "extensions": {
        "mcp": {
            "system": {
                "webhookAuthorizationLevel": "System"
            }
        }    
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    }
}